generator client {
  provider             = "prisma-client-py"
  interface            = "asyncio"
  recursive_type_depth = 5
}

datasource db {
  provider = "sqlite"
  url      = "file:../snurt.db"
}

model User {
  id         Int        @id @default(autoincrement())
  email      String
  password   String
  created_at DateTime   @default(now())
  updated_at DateTime   @updatedAt
  sessions   Sessions[]
}

model Sessions {
  id         Int      @id @default(autoincrement())
  expires_at Int
  User       User     @relation(fields: [user_id], references: [id])
  user_id    Int
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
